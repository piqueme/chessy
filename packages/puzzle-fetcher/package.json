{
  "name": "@chessy/puzzle-fetcher",
  "description": "fetch chess puzzles in bulk from major sources",
  "private": true,
  "packageManager": "yarn@3.0.2",
  "main": "build/fetcher.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./build && rm -f tsconfig.tsbuildinfo",
    "build": "yarn clean && yarn g:tsc -p tsconfig.json",
    "dev": "yarn g:tsc -b -w --preserveWatchOutput",
    "test": "yarn g:test --selectProjects @chessy/puzzle-fetcher"
  },
  "dependencies": {
    "@chessy/core": "workspace:packages/core",
    "@types/js-yaml": "^4.0.5",
    "axios": "^0.24.0",
    "chalk": "4",
    "js-yaml": "^4.1.0",
    "mkdirp": "^1.0.4",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.2",
    "@types/yargs": "^17.0.7"
  }
}
