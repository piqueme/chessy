{
  "name": "@chessy/core",
  "private": true,
  "packageManager": "yarn@3.0.2",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.2",
    "@types/uuid": "^8.3.4"
  },
  "main": "build/cjs/index.js",
  "module": "build/mjs/index.js",
  "exports": {
    ".": {
      "import": "./build/mjs/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "types": "build/mjs/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "yarn g:tsc -p tsconfig.json && yarn g:tsc -p tsconfig-cjs.json",
    "clean": "rm -rf ./build && rm tsconfig.tsbuildinfo",
    "dev": "yarn g:tsc -b -w --preserveWatchOutput",
    "test": "yarn g:test --selectProjects @chessy/core"
  },
  "dependencies": {
    "pino": "^7.6.5",
    "uuid": "^8.3.2"
  }
}
